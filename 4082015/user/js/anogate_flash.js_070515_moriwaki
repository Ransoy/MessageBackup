document.write('<object width="632" height="382" data="./images/anogate.swf" type="application/x-shockwave-flash">');
document.write('	<param value="./images/anogate.swf" name="movie">');
document.write('	<param name="quality" value="high">');
document.write('	<param name="bgcolor" value="#ffffff">');
document.write('</object>');